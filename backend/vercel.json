{
  "version": 2,
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "engines": {
    "node": "18.x"
  },
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@2.15.0"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}
